<h3>Edit your Profile</h3>
<%= form_for [@profile] do |f| %>

  <%= f.label :bio %>
  <%= f.text_field :bio, class: 'form-control' %><br>

  <%= f.label :link %>
  <%= f.text_field :link, class: 'form-control' %><br>

<br>
<div class="form-group">
     <%= f.submit "Update", class: 'btn btn-success' %>
</div>
<% end %>

