<h3><%= link_to @user.first_name, @profile %></h3><br>
<%= image_tag @user.avatar(:thumb), class: "img-circle"%></br>

<%= @profile.bio %><br>
<%= @profile.link %><br>

<% @photos.each do |photo| %>
    <h6><%= photo.description %></h6>
    <%= link_to profile_photo_path(@profile, photo) do %>
      <%= image_tag photo.image.url(:thumb)%><br>
    <% end %>
    <%= time_ago_in_words(photo.created_at) %> ago<br><br>
<% end %><br>
