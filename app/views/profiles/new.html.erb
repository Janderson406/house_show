<%= form_for @profile, url: profiles_path, html: { multipart: true } do |f| %>

  <div><%= f.label :bio %><br />
  <%= f.text_field :bio%></div>

  <div><%= f.label :link %><br />
  <%= f.text_field :link %></div>

  <div><%= f.label :avatar %><br />
  <%= f.file_field :avatar %></div>

  <div><%= f.submit "Create Profile"%></div>
<% end %>
